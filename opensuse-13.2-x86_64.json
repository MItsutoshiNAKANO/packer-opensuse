{
  "builders": [
    {
      "accelerator": "kvm",
      "boot_command": [
        "<esc><enter><wait>",
        "linux netsetup=dhcp lang=en_US hostname=build netdevice=ens3",
        " autoyast=http://{{.HTTPIP}}:{{.HTTPPort}}/autoinst-13.2.xml",
        " textmode=1 insecure=1 install=http://download.opensuse.org/distribution/13.2/repo/oss/<wait><enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_interface": "scsi",
      "disk_size": 5000,
      "format": "raw",
      "headless": true,
      "http_directory": "http",
      "iso_checksum": "f2f0ecc6b1939d3725f22221baed237f",
      "iso_checksum_type": "md5",
      "iso_url": "http://download.opensuse.org/distribution/13.2/iso/openSUSE-13.2-NET-x86_64.iso",
      "net_device": "virtio-net",
      "output_directory": "output/opensuse-13.2-x86_64-qemu",
      "qemuargs": [
        [
          "-device",
          "virtio-scsi-pci,id=scsi0"
        ],
        [
          "-drive",
          "if=none,cache=unsafe,id=drive0,discard=unmap,file=output/opensuse-13.2-x86_64-qemu/opensuse-13.2-x86_64.raw"
        ],
        [
          "-device",
          "scsi-hd,bus=scsi0.0,drive=drive0"
        ],
        [
          "-device",
          "virtio-net,netdev=user.0"
        ]
      ],
      "shutdown_command": "shutdown -h now",
      "ssh_password": "packer",
      "ssh_username": "root",
      "ssh_wait_timeout": "10000s",
      "type": "qemu",
      "vm_name": "opensuse-13.2-x86_64"
    }
  ],
  "provisioners": [
    {
      "scripts": [
        "scripts/cleanup.sh"
      ],
      "type": "shell"
    }
  ],
  "post-processors": [
    {
      "type": "compress",
      "format": "pgzip",
      "output": "output/opensuse-13.1-x86_64-compress/"
    }
  ]
}
